mixin js-loader(scriptRefs)
 script(id="system-loader" src= "https://unpkg.com/systemjs@0.21.5/dist/system-production.js")
 script(id="system-mapper") SystemJS.config(!{JSON.stringify(global.loaderConfig, null, 2)})
 each script in scriptRefs
  script System.import('#{global.static('/assets/js' + script)}')
